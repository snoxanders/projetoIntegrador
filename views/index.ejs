<html lang="pt-BR">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="icon" href="images/logos/gamer-logo-about 1.png">
    <link rel="stylesheet" type="text/css" href="stylesheets/styleHeaderFooter.css" />

    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css" integrity="sha512-9usAa10IRO0HhonpyAIVpjrylPvoDwiPUiKdWk5t3PyolY1cOd4DSE0Ga+ri4AuTroPR5aQvXU9xC6qOPnzFeg==" crossorigin="anonymous" referrerpolicy="no-referrer"
    />


    <title>Foxxer - de gamers para gamers</title>
</head>

<header>
    <%- include('./partials/navheader.ejs') %>

</header>


<body>
    <main>

        <div class="div-mae">
            <!-- banner 1 -->
            <div class="banner1">
                <img src="/public/images/home/banner1.png" alt="mulher com notebook" id="banner-image">
                <span id="title1">O computador dos seus sonhos a um clique de distância</span>
            </div>
            <!-- banner 2 -->
            <div class="banner2">
                <img src="/public/images/home/image1.png" alt="dois notebooks">
                <span id="title2">Notebooks de ponta pra suprir sua demanda</span>
                <h5>Lorem ipsum dolor sit amet, consectetur adipiscing elit.Felis eget velit aliquet sagittis id consectetur purus ut.</h5>
                <div class="button-div"><button id="button" type="button">Empresarial</button><button type="button" id="button">Pessoal</button></div>
            </div>
            <!-- banner 3 -->
            <div class="banner3">
                <span id="title3">NOTEBOOKS DE PONTA PRA SUPRIR A SUA DEMANDA****</span>
                <div class="card">
                    <span id="title-card">Planos a partir de</span>
                    <span id="value-card">R$490,00</span>
                    <P id="text-card">por mês</P>
                    <span id="line-card"><img src="/public/images/home/imgcard1.png" alt="img1">Lorem ipsum dolor sit amet</span>
                    <span id="line-card"><img src="/public/images/home/imgcard2.png" alt="img2">Lorem ipsum dolor sit amet</span>
                    <span id="line-card"><img src="/public/images/home/imgcard3.png" alt="img3">Lorem ipsum dolor sit amet</span>
                    <button type="button" id="button-card">Quero esse</button>
                </div>
                <div class="card">
                    <span id="title-card">Planos a partir de</span>
                    <span id="value-card">R$380,00</span>
                    <P id="text-card">por mês</P>
                    <span id="line-card"><img src="/public/images/home/imgcard1.png" alt="img1">Lorem ipsum dolor sit amet</span>
                    <span id="line-card"><img src="/public/images/home/imgcard2.png" alt="img2">Lorem ipsum dolor sit amet</span>
                    <span id="line-card"><img src="/public/images/home/imgcard3.png" alt="img3">Lorem ipsum dolor sit amet</span>
                    <span id="line-card"><img src="/public/images/home/imgcard4.png" alt="img4">Lorem ipsum dolor sit amet</span>
                    <span id="line-card"><img src="/public/images/home/imgcard5.png" alt="img5">Lorem ipsum dolor sit amet</span>
                    <button type="button" id="button-card">Quero esse</button>
                </div>
                <!-- mini carrossel -->
                <div class="carroussel"><img src="/public/images/home/carroussel.png" alt=""></div>
                <!-- continuação do banner 3 -->
                <div class="div-filha">
                    <div class="div-neta1">
                        <span id="title-divneta1">Facilidade e qualidade: Tenha o melhor equipamentono menor tempo.</span>
                        <p id="text-divneta1">Lorem ipsum dolor sit amet, consectetur adipiscing elit.Felis eget velit aliquet sagittis id consectetur purus ut.</p>
                    </div>
                    <div class="div-neta2">
                        <span id="title-divneta2">Macbook Pro</span>
                        <img id="img-divneta2" src="/public/images/home/image2.png" alt="dois notebooks">
                    </div>
                </div>
                <!-- svg detalhe -->
                <svg width="1920" height="120" viewBox="0 0 1920 120" fill="none" xmlns="http://www.w3.org/2000/svg">
                        <path d="M1920 120L0 16.48V0H1920V120Z" fill="#F9FAFB"/>
                        </svg>
            </div>
            <!-- banner 4 -->
            <div class="banner4">
                <div class="div-form">
                    <form action="POST">
                        <legend id="legend-form">Solicite seu orçamento</legend>
                        <p>
                            <input type="text" name="name" id="name" placeholder="Seu nome completo">
                            <input type="email" name="email" id="email" placeholder="Seu e-mail">
                            <input type="tel" name="phone" id="phone" placeholder="Seu telefone">
                            <input type="number" name="cep" id="cep" required pattern="\d{5}-\d{3}" placeholder="CEP do local da entrega">

                        </p>

                        <span id="title-form">Por quanto tempo você precisa?</span>

                        <input type="radio" name="checkbox"><label id="label-form">1 a 3 meses</label>
                        <input type="radio" name="checkbox"><label id="label-form">3 meses</label>
                        <input type="radio" name="checkbox"><label id="label-form">6 meses</label>
                        <input type="radio" name="checkbox"><label id="label-form">Mais de um ano</label>
                        <br>
                        <input type="text" id="message-form" name="messageform" placeholder="Nos conte o que você precisa">

                        <button type="submit" id="button-form">Enviar</button>

                    </form>
                </div>
                <div class="imgform">
                    <img id="imgform" src="/public/images/home/image3.png" alt="mulher loira no notebook">
                </div>
            </div>
        </div>
    </main>

    <script type="text/javascript" src="javascripts/mobile-navbar.js"></script>
</body>


<footer>
    <%- include('./partials/footer.ejs') %>
</footer>

</html>